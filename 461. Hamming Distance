public class Solution {    public int hammingDistance(int x, int y) {        int mod1,mod2;        int count=0;        int max=x>y?x:y;        while(max>0){            mod1=x%2;            x=x/2;            mod2=y%2;            y=y/2;            max=max/2;            if(mod1!=mod2){count++;}        }        return count;    }}
